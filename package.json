{
  "name": "no.runely.jslogic",
  "version": "0.0.1",
  "main": "app.js",
  "devDependencies": {
    "homey": "2.9.4",
    "xo": "0.37.1"
  },
  "dependencies": {
    "@sentry/node": "5.29.2",
    "@sentry/tracing": "5.29.2",
    "moment": "2.29.1"
  },
  "scripts": {
    "refresh": "rm -rf node_modules/ && rm package-lock.json && npm i",
    "test": "xo",
    "fix": "xo --fix",
    "homey-publish": "npm run fix && homey app publish"
  },
  "xo": {
    "space": 2,
    "semicolon": true,
    "ignores": [],
    "rules": {
      "no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "args": "after-used"
        }
      ],
      "object-curly-spacing": "off",
      "eol-last": "off",
      "capitalized-comments": "off",
      "no-negated-condition": "off",
      "radix": "warn"
    }
  }
}
